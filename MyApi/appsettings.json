{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "domainName_here",
    "TenantId": "tanenetId_here",
    "ClientId": "clientId_here",
    "CallbackPath": "/callback_path",
    "SignedOutCallbackPath ": "/sign_outPath_here",
    "ClientSecret": "client_secret"
  },
  "CallApi": {
    "ScopeForAccessToken": "api://access_token_here",
    "ApiBaseAddress": "https://localApiURL"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
